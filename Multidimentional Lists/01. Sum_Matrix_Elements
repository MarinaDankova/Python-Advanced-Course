rows, cols = [int(el) for el in input().split(', ')]

matrix = []
result = 0

for row in range(rows):
    matrix.append([int(el) for el in input().split(',')])
    result += sum(matrix[row])

# or with adding second cycle:
# for row in range(rows):
#     for col in range(cols):
#         result+=matrix[row][col]
#
# print(result)
# print(matrix)


print(result)
print(matrix)
